@using SMARTCUSTOMERORDER.Models;
@model UserContactDetails
@{
    ViewData["Title"] = "UserList";
}
<div class="panel panel-primary">
    <div class="panel-heading">
        UserDetailsList
    </div>
    <div class="panel-body">
        <div>
            <table class="table table-striped table-bordered" id="TblUserList">
                <thead>
                    <tr>
                        <td>SNo</td>
                        <td>Name</td>
                        <td>Surname</td>
                        <td>Gender</td>
                        <td>Email</td>
                        <td>Edit / Delete</td>
                    </tr>
                </thead>
                <tbody>
                    @foreach (var item in Model.UserDataTableList)
                    {
                        <tr>
                            <td>@item.SNo</td>
                            <td>@item.Name</td>
                            <td>@item.Surname</td>
                            <td>@item.Gender</td>
                            <td>@item.Email</td>
                            <td>
                                <a onclick="Edit(@item.SNo)">Edit</a> |
                                <a onclick="Delete(@item.SNo)">Delete</a>
                            </td>
                        </tr>
                    }
                </tbody>
            </table>
        </div>
    </div>
</div>



